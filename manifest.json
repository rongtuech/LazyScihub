{
	"manifest_version":2,
	"name": "lazy scihub",
	"version":"0.1",
	"content_scripts": [
		  {
		    "matches": [
		      "<all_urls>"
		    ],
		    "js": ["jquery-3.3.1.min.js","content.js"]
		  }
	],
	"background": {
      "scripts": ["background.js"]
    },
	"browser_action":{
		"default_icon": "lazy.png"
	},
	"permissions": [
      "activeTab"
    ]
}